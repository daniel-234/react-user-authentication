{
  "name": "react-user-authentication",
  "version": "1.0.0",
  "description": "A user authentication application built with React and JWT (Jason Web Tokens)",
  "scripts": {
    "build": "webpack",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "precommit": "lint-staged",
    "watch": "webpack --watch"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-234/react-user-authentication.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniel-234/react-user-authentication/issues"
  },
  "homepage": "https://github.com/daniel-234/react-user-authentication#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
